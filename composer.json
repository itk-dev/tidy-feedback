{
    "name": "itk-dev/tidy_feedback",
    "description": "Tidy feedback",
    "license": "MIT",
    "type": "drupal-module",
    "require": {
        "doctrine/dbal": "^3 || ^4",
        "doctrine/orm": "^2.8 || ^3",
        "symfony/cache": "^6 || ^7",
        "twig/twig": "^3"
    },
    "require-dev": {
        "drupal/coder": "^8.3",
        "ergebnis/composer-normalize": "^2.47",
        "friendsofphp/php-cs-fixer": "^3.75",
        "rector/rector": "^2.0",
        "vincentlanglet/twig-cs-fixer": "^3.8"
    },
    "autoload": {
        "psr-4": {
            "ItkDev\\TidyFeedbackBundle\\": "symfony/src/",
            "ItkDev\\TidyFeedback\\": "src/"
        }
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "ergebnis/composer-normalize": true
        }
    }
}
